let childHeight = 4

function displayIfChildIsAbleToRideTheRollerCoaster() {
    //If the childHeight is greater than 52, the console.log should say "Get on that ride, kiddo!".
    if (childHeight > 52) {
        console.log("Get on that ride, kiddo!")
    }
    //Else, console.log should say "Sorry kiddo. Maybe next year."
    else {
        console.log("Sorry kiddo. Maybe next year.")
    }
}
